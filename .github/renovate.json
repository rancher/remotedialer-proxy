{
  "extends": [
    "github>rancher/renovate-config#release"
  ],
  "baseBranchPatterns": [
    "main"
  ],
  "prHourlyLimit": 2,
  "packageRules": [
    {
      "enabled": false,
      "matchPackageNames": [
        "/k8s.io/*/",
        "/sigs.k8s.io/*/",
        "/github.com/prometheus/*/"
      ]
    },
    {
      "matchPackageNames": ["golang"],
      "updateTypes": ["patch"]
    }
  ]
}
